{
  "name": "@crishellco/vue-renderless-party",
  "version": "0.5.0",
  "author": "Chris Mitchell (@crishellco)",
  "description": "A collection of useful Vue renderless components",
  "main": "dist/vue-renderless-party.umd.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/demo/index.js --open",
    "build": "vue-cli-service build --target lib src/index.js",
    "build:demo": "vue-cli-service build src/demo/index.js --dest dist-demo",
    "test": "yarn test:unit && jest-coverage-badges output badges",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crishellco/vue-renderless-party.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crishellco/vue-renderless-party/issues"
  },
  "homepage": "https://crishellco.github.io/vue-renderless-party/",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "fuse.js": "^6.4.6",
    "sort-array": "^4.1.4",
    "vue": "^2.6.11",
    "vue-code-highlight": "^0.7.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "jest-coverage-badges": "^1.1.2",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "pre-push": "yarn test:unit --coverage=false"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
